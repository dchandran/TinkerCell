<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ConnectionsTree>
<ConnectionsTree version="1.0">
<connection family="Biochemical" participant_types="Node,Node" participant_roles="Reactant,Product">
<text>This is a generic class representing any biological reaction with reactant(s) and/or product(s).</text>
    
    <connection family="1 to 1" participant_types="Node,Node" participant_roles="Reactant,Product">

      <connection family="Degradation" participant_types="Molecule,Empty" participant_roles="Reactant,Empty">
        <text>This connection represents degradation, dilution, or secretion of a molecular species.</text>
      </connection>

      <connection family="Production" participant_types="Empty,Molecule" participant_roles="Empty,Product">
        <text>This connection represents production or influx of a molecular species.</text>
        <connection family="Transcription" participant_types="Empty,mRNA" participant_roles="Empty,Product">
          <text>Production of mRNA from DNA template.</text>
        </connection>
        <connection family="Translation" participant_types="Empty,Protein" participant_roles="Empty,Product">
          <text>Production of protein from mRNA template. </text>
        </connection>
      </connection>
      
      <connection family="Protein production" participant_types="Coding,mRNA" participant_roles="Template,Product">
        <text>The complete process of producing a protein from the template DNA.</text>
      </connection>
    
    </connection>

    <connection family="2 to 1" participant_types="Node" participant_roles="Reactant1,Reactant2,Product"></connection>
    <connection family="3 to 1" participant_types="Node" participant_roles="Reactant1,Reactant2,Reactant3,Product"></connection>
    <connection family="1 to 2" participant_types="Node" participant_roles="Reactant1,Product,Product"></connection>
    <connection family="2 to 2" participant_types="Node" participant_roles="Reactant,Reactant,Product,Product"></connection>
    <connection family="3 to 2" participant_types="Node" participant_roles="Reactant,Reactant,Reactant,Product,Product"></connection>
    <connection family="1 to 3" participant_types="Node" participant_roles="Reactant,Product,Product,Product"></connection>
    <connection family="2 to 3" participant_types="Node" participant_roles="Reactant,Reactant,Product,Product,Product"></connection>
    <connection family="3 to 3" participant_types="Node" participant_roles="Reactant,Reactant,Reactant,Product,Product,Product"></connection>

    <connection family="Regulation" participant_types="Node" participant_roles="Regulator,Target">    
      <text>This connection represents a molecule regulating another.</text>
      
      <connection family="Enzyme Catalysis" participant_types="Protein,Molecule,Molecule" participant_roles="Catalyst,Reactant,Product">
      </connection> 
      
      <connection family="Activation" participant_types="Molecule,Node">
          <connection family="Transcription Activation" participant_types="Transcription Factor,Inducible Promoter">
          </connection> 
      </connection>
      
      <connection family="Repression" participant_types="Molecule,Node">
        <connection family="Transcription Repression" participant_types="Transcription Factor,Repressible Promoter">
        </connection>
      </connection>

      <connection family="Direct Gene Regulation" participant_types="Coding,Promoter">
        <text>This connection represents a gene regulating another gene.</text>
        <connection family="Direct Gene Activation" participant_types="Coding,Inducible Promoter">
        </connection>
  	    <connection family="Direct Gene Repression" participant_types="Coding,Repressible Promoter">
        </connection>
      </connection>
    </connection>
  <connection family="PoPS" participant_types="Part,Part">
    <text>This connection represents the movement of RNA polymerase along a strand of DNA.</text>
  </connection>  
</connection>
</ConnectionsTree>

